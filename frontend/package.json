{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install frontend/.husky; fi",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "preview": "vite preview",
    "storybook": "cross-env NODE_ENV=development storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "cypress:verify": "cypress verify",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:clear": "cypress cache clear"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "prettier --write"
    ]
  },
